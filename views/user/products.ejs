	<script>										$(document).ready(function () {
			$('#people').on('change', function () {
				var sortVal = $(this).val();
				var categoryId = '{{categoryId}}';
				var url;
				if (sortVal == '-1') {
					url = '/sort?id=' + categoryId + '&sort=-1';
				} else if (sortVal == '1') {
					url = '/sort?id=' + categoryId + '&sort=1';
				}
