<script src="https://cdn.jsdelivr.net/npm/sweetalert2@10.15.6/dist/sweetalert2.all.min.js"></script>

<!-- ... Your HTML and other scripts ... -->

<script>
    const addcart = function (id, quantity) {
        if (quantity === null || quantity === undefined) {
            // Set a default quantity if 'quantity' is null or undefined
            quantity = 1;
        }

        $.ajax({
            url: "/add-to-cart/" + id,
            method: 'POST',
            data: {
                quantity: quantity
            },
            success: function (response) {
                // Handle the response from the server here
                if (response.success) {
                    // Product was successfully added to the cart
                    Swal.fire(
                        'Added to Cart!',
                        'The product has been added to your cart.',
                        'success'
                    );
                } else {
                    // Failed to add the product to the cart
                    Swal.fire(
                        'Oops!',
                        'Something went wrong. Please try again later.',
                        'error'
                    );
                }
            },
            error: function () {
                // Failed to send the AJAX request
                Swal.fire(
                    'Oops!',
                    'Failed to add the product to the cart. Please try again later.',
                    'error'
                );
            }
        });
    };

    // Add a click event listener to the element with the class "social-info"
    $(document).on('click', '.social-info', function() {
        // Get the product ID from the data attribute or any other way you're storing it
        const productId = $(this).data('product-id');

        // Call the addcart function with the product ID and default quantity of 1
        addcart(productId, 1);
    });
</script>


